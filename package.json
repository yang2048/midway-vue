{
  "name": "midway-vue",
  "version": "1.0.0",
  "description": "基于 Midway、Vue 3 组装的 SSR 框架，简单、易学易用、方便扩展、集成Midway框架，您一直想要的Vue SSR 框架。",
  "private": true,
  "dependencies": {
    "@midwayjs/bootstrap": "^3.6.0",
    "@midwayjs/core": "^3.6.0",
    "@midwayjs/decorator": "^3.6.0",
    "@midwayjs/info": "^3.6.0",
    "@midwayjs/koa": "^3.6.0",
    "@midwayjs/static-file": "^3.6.0",
    "@midwayjs/typeorm": "^3.0.0",
    "@midwayjs/validate": "^3.6.0",
    "axios": "^1.1.2",
    "koa-connect": "^2.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.22",
    "typeorm": "^0.3.10",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@midwayjs/cli": "^1.3.15",
    "@midwayjs/mock": "^3.6.0",
    "@types/jest": "^29.1.1",
    "@types/koa": "^2.13.5",
    "@types/node": "^18.8.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "cross-env": "^7.0.3",
    "jest": "^29.1.2",
    "mwts": "^1.3.0",
    "naive-ui": "^2.33.4",
    "rollup-plugin-analyzer": "^4.0.0",
    "sass": "^1.55.0",
    "terser": "^5.15.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^18.8.0",
    "typescript": "~4.8.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.1.6",
    "vue-tsc": "^1.0.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./bootstrap.js",
    "dev": "cross-env TS_NODE_TYPE_CHECK=false TS_NODE_TRANSPILE_ONLY=true NODE_ENV=local midway-bin dev --ts",
    "build": "pnpm run clean && pnpm run build:midway && pnpm run build:web",
    "build:midway": "midway-bin build -c",
    "build:web": "pnpm run build:web:client && pnpm run build:web:server",
    "build:web:client": "vite --config view/vite.config.ts build --ssrManifest --outDir ../build/client",
    "build:web:server": "vite --config view/vite.config.ts build --ssr entry-server --outDir ../build/server",
    "clean": "midway-bin clean",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "build",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "vue3",
    "midway",
    "typescript",
    "pinia"
  ],
  "author": "yang2048<yang2048@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yang2048/midway-vue3/issues"
  },
  "homepage": "https://github.com/yang2048/midway-vue3#readme"
}